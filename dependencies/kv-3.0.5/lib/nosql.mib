   ::= { repNodeEventList 4 }

-- repNodeMultiLatencyCeilingExceeded: nosql 4.0.5
    repNodeMultiLatencyCeilingExceeded NOTIFICATION-TYPE
        OBJECTS {
            repNodeShardNumber,
            repNodeNumber,
            repNodeMultiIntervalLatAvg
        }
        STATUS          current
        DESCRIPTION
            "Sent when a Replication Node's multi-operation average latency is
            greater than the set limit. Carries a copy of the offending
            latency value."
   ::= { repNodeEventList 5 }

-- repNodeCommitLagThresholdExceeded: nosql 4.0.6
    repNodeCommitLagThresholdExceeded NOTIFICATION-TYPE
        OBJECTS {
            repNodeShardNumber,
            repNodeNumber,
            repNodeCommitLag
        }
        STATUS          current
        DESCRIPTION
            "Sent when the average commit lag (in milliseconds) for the
            associated replication node during a given time interval exceeds
            the set threshold. Carries a copy of the offending lag value."
   ::= { repNodeEventList 6 }

