-- The repNodes node contains a count of the RepNodes
-- and the Array of repnodes.

-- nRepNodes: nosql.3.1
    nRepNodes   OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The number of Replication Nodes hosted on this
                         Storage Node."
    ::= { repNodes 1 }

-- repNodesTable: nosql.3.2
    repNodeTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF RepNodeTableEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Table of Replication Nodes hosted on this Storage Node."
    ::= { repNodes 2 }

-- repNodesTable: nosql.3.2.1
    repNodeTableEntry OBJECT-TYPE
        SYNTAX          RepNodeTableEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "An entry in the repNodeTable"
        INDEX           { repNodeShardNumber,
                          repNodeNumber}
    ::= { repNodeTable 1 }

    RepNodeTableEntry ::=
        SEQUENCE {
                repNodeShardNumber                      INTEGER,
                repNodeNumber                           INTEGER,
                repNodeServiceStatus                    ServiceStatusTC,

                -- Single Interval Latency Info
                repNodeIntervalStart                    DateAndTime,
                repNodeIntervalEnd                      DateAndTime,
                repNodeIntervalPeriod                   TimeInterval,
                repNodeIntervalTotalOps                 INTEGER,
                repNodeIntervalThroughput               Unsigned32,
                repNodeIntervalLatMin                   INTEGER,
                repNodeIntervalLatMax                   INTEGER,
                repNodeIntervalLatAvg                   DisplayString,
                repNodeIntervalLatAvgInt                INTEGER,
                repNodeIntervalLatAvgFrac               INTEGER,
                repNodeIntervalPct95                    INTEGER,
                repNodeIntervalPct99                    INTEGER,

                -- Single Cumulative Latency Info
                repNodeCumulativeStart                  DateAndTime,
                repNodeCumulativeEnd                    DateAndTime,
                repNodeCumulativeTotalOps               INTEGER,
                repNodeCumulativeThroughput             Unsigned32,
                repNodeCumulativeLatMin                 INTEGER,
                repNodeCumulativeLatMax                 INTEGER,
                repNodeCumulativeLatAvg                 DisplayString,
                repNodeCumulativeLatAvgInt              INTEGER,
                repNodeCumulativeLatAvgFrac             INTEGER,
                repNodeCumulativePct95                  INTEGER,
                repNodeCumulativePct99                  INTEGER,

                -- Multi Op Interval Latency Info
                repNodeMultiIntervalStart               DateAndTime,
                repNodeMultiIntervalEnd                 DateAndTime,
                repNodeMultiIntervalPeriod              TimeInterval,
                repNodeMultiIntervalTotalOps            INTEGER,
                repNodeMultiIntervalTotalRequests       INTEGER,
                repNodeMultiIntervalThroughput          Unsigned32,
                repNodeMultiIntervalLatMin              INTEGER,
                repNodeMultiIntervalLatMax              INTEGER,
                repNodeMultiIntervalLatAvg              DisplayString,
                repNodeMultiIntervalLatAvgInt           INTEGER,
                repNodeMultiIntervalLatAvgFrac          INTEGER,
                repNodeMultiIntervalPct95               INTEGER,
                repNodeMultiIntervalPct99               INTEGER,

                -- Multi Op Cumulative Latency Info
                repNodeMultiCumulativeStart             DateAndTime,
                repNodeMultiCumulativeEnd               DateAndTime,
                repNodeMultiCumulativeTotalOps          INTEGER,
                repNodeMultiCumulativeTotalRequests     INTEGER,
                repNodeMultiCumulativeThroughput        Unsigned32,
                repNodeMultiCumulativeLatMin            INTEGER,
                repNodeMultiCumulativeLatMax            INTEGER,
                repNodeMultiCumulativeLatAvg            DisplayString,
                repNodeMultiCumulativeLatAvgInt         INTEGER,
                repNodeMultiCumulativeLatAvgFrac        INTEGER,
                repNodeMultiCumulativePct95             INTEGER,
                repNodeMultiCumulativePct99             INTEGER,

                -- Commit Lag
                repNodeCommitLag                        Unsigned32,

                -- RepNode Parameters
                repNodeCacheSize                        INTEGER,
                repNodeConfigProperties                 DisplayString,
                repNodeCollectEnvStats                  TruthValue,
                repNodeStatsInterval                    INTEGER,
                repNodeMaxTrackedLatency                INTEGER,
                repNodeJavaMiscParams                   DisplayString,
                repNodeLoggingConfigProps               DisplayString,
                repNodeHeapSize                         INTEGER,
                repNodeMountPoint                       DisplayString,
                repNodeLatencyCeiling                   INTEGER,
                repNodeThroughputFloor                  INTEGER,
                repNodeCommitLagThreshold               Unsigned32
        }

