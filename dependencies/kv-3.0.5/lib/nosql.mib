OracleNosqlMIB DEFINITIONS ::= BEGIN

-- MIB implemented by the Oracle Nosql Database's Storage Node Agent.
-- This SNMP agent represents the Storage Node and the Storage Node's
-- managed processes.
--
-- The abbreviation "nosql," as used in comments mentioning OIDs, is defined as
-- iso(1).org(3).dod(6).internet(1).private(4).enterprises(1).
--     oracle(111).nosql(42)

IMPORTS
    MODULE-IDENTITY, OBJECT-IDENTITY, NOTIFICATION-TYPE, enterprises,
        OBJECT-TYPE, Unsigned32, Integer32, Counter32
        FROM SNMPv2-SMI

    OBJECT-GROUP, NOTIFICATION-GROUP, MODULE-COMPLIANCE
        FROM SNMPv2-CONF

    DisplayString, TruthValue, TEXTUAL-CONVENTION, TimeInterval, DateAndTime
        FROM SNMPv2-TC;

    nosql MODULE-IDENTITY
        LAST-UPDATED "201402070000Z"
        ORGANIZATION "Oracle Corporation"
        CONTACT-INFO "support@sleepycat.com"
        DESCRIPTION
            "This MIB defines SNMP data and trap objects reported by
             Oracle Nosql Database's Storage Node Agent."
        REVISION "201402070000Z"
        DESCRIPTION "Oracle NoSQL Database 3.0"
    ::= { oracle 42 }

-- External OIDs

    oracle      OBJECT IDENTIFIER ::= { enterprises 111 }

-- Textual Conventions (typedefs)

ServiceStatusTC ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Enumeration of service status"
    SYNTAX INTEGER {
        starting(1),
        waitingForDeploy(2),
        running(3),
        stopping(4),
        stopped(5),
        errorRestarting(6),
        errorNoRestart(7),
        unreachable(8)
    }

-- Top-level component categories

    storageNode       OBJECT IDENTIFIER ::= { nosql 1 }
    storageNodeEvents OBJECT IDENTIFIER ::= { nosql 2 }
    repNodes          OBJECT IDENTIFIER ::= { nosql 3 }
    repNodeEvents     OBJECT IDENTIFIER ::= { nosql 4 }
    admin             OBJECT IDENTIFIER ::= { nosql 5 }
    adminEvents       OBJECT IDENTIFIER ::= { nosql 6 }

