### Begin Script ###
configure -name tagstore
plan deploy-zone -name "Rex" -rf 1 -wait
plan deploy-sn -zn zn1 -host localhost -port 5000 -wait
plan deploy-admin -sn sn1 -port 5001 -wait
pool create -name RexPool
pool join -name RexPool -sn sn1
topology create -name topo -pool RexPool -partitions 10
plan deploy-topology -name topo -wait
exit
### End Script ### 

### run with ###
# java -Xmx256m -Xms256m -jar $KVHOME/lib/kvstore.jar runadmin -port 5000 -host localhost load -file database/dbsetup